        <a href="/" class="nav_logo_movil text-black ">
            <div class="div_nav_icons_movil">
                <div class="icon_nav">
                    <IconCarrito/>
                </div>
                <div>
                    Carrito
                </div>                
            </div>
        </a>
        <a href="/" class="nav_logo_movil text-black ">
             
             <div class="div_nav_icons_movil">
                <div class="icon_nav">
                    <IconIniciarSesion/>
