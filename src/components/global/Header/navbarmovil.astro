        <a href="/" class="nav_logo_movil text-black ">
            <LogoMovil/>
        </a>
        <a href="/" class="nav_logo_movil text-black ">
            <div class="div_nav_icons_movil">
                <div class="icon_nav">
                <IconBuscador/>
                </div>
                <div class="title_icon_nav">
                    Buscar
                </div>                
            </div>
        </a>
        <a href="/" class="nav_logo_movil text-black ">           
            <div class="div_nav_icons_movil">
                <div class="icon_nav">
                    <IconFotosVideos/>
                </div>
                <div>
                    Media
                </div>                
            </div>
